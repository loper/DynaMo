+for op, arg in OPCJE:
+    if op in ('-v', '--verbose'):
+        tryb_verbose(True)
 
-#os.system('clear')
-print 20 * "\n"
+os.system('clear')
+#print 20 * "\n"
 print KONF.podaj_wartosc("naglowek")
 print "wersja %s by %s" % (KONF.podaj_wartosc("wersja"),
                            KONF.podaj_wartosc("autor"))
